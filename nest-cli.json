{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/exercicio-1/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      "@nestjs/swagger"
    ],
    "webpack": true,
    "tsConfigPath": "apps/exercicio-1/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/exercicio-1",
  "projects": {
    "exercicio-1": {
      "type": "application",
      "root": "apps/exercicio-1",
      "entryFile": "main",
      "sourceRoot": "apps/exercicio-1/src",
      "compilerOptions": {
        "tsConfigPath": "apps/exercicio-1/tsconfig.app.json"
      }
    },
    "exercicio-2": {
      "type": "application",
      "root": "apps/exercicio-2",
      "entryFile": "main",
      "sourceRoot": "apps/exercicio-2/src",
      "compilerOptions": {
        "tsConfigPath": "apps/exercicio-2/tsconfig.app.json"
      }
    }
  }
}
